# Exemplo de arquivo de variáveis de ambiente
# Copie este arquivo para .env e ajuste os valores conforme necessário

# Configurações de ambiente
PYTHONPATH=src
ENVIRONMENT=development

# Configurações de logging
LOG_LEVEL=INFO
LOG_FILE=results/logs/app.log

# Configurações de dados
DATA_PATH=data
RAW_DATA_PATH=data/raw
PROCESSED_DATA_PATH=data/processed

# Configurações de modelos
MODELS_PATH=models
CHECKPOINTS_PATH=models/checkpoints
TRAINED_MODELS_PATH=models/trained

# Configurações de resultados
RESULTS_PATH=results
PLOTS_PATH=results/plots
LOGS_PATH=results/logs
EXPERIMENTS_PATH=results/experiments

# Configurações de ML
RANDOM_SEED=42
NUM_EPISODES=10000
BATCH_SIZE=256
LEARNING_RATE=0.001

# Configurações de hardware
CUDA_VISIBLE_DEVICES=0
OMP_NUM_THREADS=4